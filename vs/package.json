{
  "name": "otto",
  "displayName": "otto",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "otto.deautomate",
        "title": "Open Deautomation Panel"
      }
    ],
    "keybindings": [
      {
        "key": "alt+;",
        "mac": "ctrl+alt+;",
        "command": "otto.deautomate"
      }
    ],
    "configuration": {
      "title": "Deautomation",
      "properties": {
        "otto.coqtop": {
          "type": "string",
          "default": "coqtop",
          "description": "Path to coqtop"
        }
      }
    }
  },
  "scripts": {
    "install:all": "npm install && cd otto-ui && npm install",
    "build:all": "cd otto-ui && npm run build",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "dependencies": {
    "antlr4": "^4.13.0",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.24",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}